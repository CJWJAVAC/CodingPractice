import java.util.Scanner;

public class Main {

	public static void main(String[] args) {
		Scanner scan = new Scanner(System.in);
		int L = scan.nextInt();
		int P = scan.nextInt();
		int a = scan.nextInt();
		int b = scan.nextInt();
		int c = scan.nextInt();
		int d = scan.nextInt();
		int e = scan.nextInt();
		int count=L*P;
		int A=a-count;
		int B=b-count;
		int C=c-count;
		int D=d-count;
		int E=e-count;
		System.out.println(A+" "+B+" "+C+" "+D+" "+E);
	}

}
